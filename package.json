{
  "name": "mstream-node",
  "version": "1.0.0",
  "description": "node streaming api",
  "author": "Bhanu Pratap Singh",
  "license": "ISC",
  "main": "server.js",
  "scripts": {
    "build": "npm run build-ts",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon -r dotenv/config dist/server.js",
    "node": "node dist/server.js",
    "dev": "concurrently -i -n \"TS,NODE\" -c \"bgMagenta.bold,bgCyan.bold\" \"npm run watch-ts\" \"npm run watch-node\" ",
    "start": "npm run build && npm run node"
  },
  "engines": {
    "node": "12.18.3"
  },
  "dependencies": {
    "@types/mongodb": "^3.5.33",
    "@types/mongoose": "^5.10.0",
    "express": "^4.16.3",
    "mongodb": "^3.6.3",
    "mongoose": "^5.10.14",
    "typescript": "^4.0.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/node": "^14.0.27",
    "@types/shelljs": "^0.8.8",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "concurrently": "^5.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.0.5"
  }
}
