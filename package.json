{
    "name": "mstream-node",
    "version": "1.0.0",
    "description": "node streaming api",
    "author": "Bhanu Pratap Singh",
    "license": "ISC",
    "main": "server.js",
    "type": "module",
    "scripts": {
        "build": "npm run build-ts",
        "build-ts": "tsc",
        "watch-ts": "tsc -w",
        "watch-node": "cross-env NODE_ENV=development nodemon --experimental-specifier-resolution=node -r dotenv/config dist/server.js",
        "node": "node --experimental-specifier-resolution=node dist/server.js",
        "dev": "concurrently -i -n \"TS,NODE\" -c \"bgMagenta.bold,bgCyan.bold\" \"npm run watch-ts\" \"npm run watch-node\" ",
        "start": "npm run build && npm run node",
        "start:dev": "npm run build && npm run node"
    },
    "engines": {
        "node": "14.15.4"
    },
    "dependencies": {
        "@typegoose/typegoose": "^7.4.7",
        "@types/cors": "^2.8.8",
        "@types/express": "^4.17.9",
        "@types/mongodb": "^3.5.33",
        "@types/mongoose": "^5.10.0",
        "@types/morgan": "^1.9.2",
        "@types/node": "^14.14.7",
        "apollo-server-express": "^2.19.1",
        "class-validator": "^0.13.0",
        "cors": "^2.8.5",
        "express": "^4.16.3",
        "express-async-handler": "^1.1.4",
        "graphql": "^15.4.0",
        "mongodb": "^3.6.3",
        "mongoose": "^5.10.14",
        "morgan": "^1.10.0",
        "node-id3": "^0.2.2",
        "reflect-metadata": "^0.1.13",
        "type-graphql": "^1.1.1",
        "typescript": "^4.1.3"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^3.7.1",
        "@typescript-eslint/parser": "^3.7.1",
        "concurrently": "^5.2.0",
        "cross-env": "^7.0.3",
        "dotenv": "^8.2.0",
        "eslint": "^7.6.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.4",
        "nodemon": "^2.0.7",
        "prettier": "^2.0.5"
    }
}
